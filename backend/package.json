{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "deploy": "strapi deploy",
    "develop": "strapi develop NODE_ENV=development",
    "start": "strapi start",
    "strapi": "strapi",
    "seed:users": "node seeder/users-create.seeder.mjs",
    "seed:posts": "node seeder/posts-create.seeder.mjs",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.1.0",
    "@strapi/plugin-graphql": "^5.1.0",
    "@strapi/plugin-users-permissions": "5.1.0",
    "@strapi/strapi": "5.1.0",
    "better-sqlite3": "^11.4.0",
    "pg": "8.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "b5b38e1d-c84e-4f79-a1af-024557aaaa3c"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".tmp",
      ".cache"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".tmp",
      ".cache"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "node"
    ]
  }
}
